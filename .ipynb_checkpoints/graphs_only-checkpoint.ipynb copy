    "def add_rho(row):\n",
    "    state = row['state']\n",
    "    rho = state_densities[state_densities['state'] == state]['propotionate_log_density']\n",
    "    return rho.values[0]\n",
    "\n",
    "weekly_est_cases_deaths['rho'] =  weekly_est_cases_deaths.apply(add_rho, axis=1)\n",
    "\n",
    "weekly_est_cases_deaths.head(5)"
   ]
